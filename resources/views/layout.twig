<!DOCTYPE html>
<html itemscope itemtype="http://schema.org/NewsArticle">
  <head>
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">
    <link rel="stylesheet" type="text/css" href="{{ assetPath("application.css") }}"/>
    <link href="https://d2nx6ydw3e5y5d.cloudfront.net/assets/current.css" media="screen" rel="stylesheet" type="text/css">
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
    <script type="text/javascript" src="//cdn.jsdelivr.net/jquery.slick/1.6.0/slick.min.js"></script>
    <script type="text/javascript" src="{{ assetPath("application.js") }}"></script>
    <meta name="twitter:widgets:csp" content="on">
    {{ meta.display([], true)|raw }}


    <script src='https://www.googletagservices.com/tag/js/gpt.js'></script>
    <script>
        var googletag = googletag || {};
        googletag.cmd = googletag.cmd || [];
      </script>
    <script>
      googletag.cmd.push(function() {
        googletag.defineSlot('/60988533/Rio_HP_LB1', [[728, 90], [320, 50]], 'div-gpt-ad-1485772432963-0').addService(googletag.pubads()).setTargeting('demo', '{{ app.request.get("demo") }}');
        googletag.defineSlot('/60988533/Rio_HP_LB2', [[728, 90], [320, 50]], 'div-gpt-ad-1485772432963-1').addService(googletag.pubads()).setTargeting('demo', '{{ app.request.get("demo") }}');
        googletag.defineSlot('/60988533/Rio_HP_Mrec1', [300, 250], 'div-gpt-ad-1485772432963-2').addService(googletag.pubads()).setTargeting('demo', '{{ app.request.get("demo") }}');
        googletag.defineSlot('/60988533/Rio_HP_Mrec2', [300, 250], 'div-gpt-ad-1485772432963-3').addService(googletag.pubads()).setTargeting('demo', '{{ app.request.get("demo") }}');
        googletag.defineSlot('/60988533/Rio_Section_300x600', [300, 600], 'div-gpt-ad-1485772432963-4').addService(googletag.pubads()).setTargeting('demo', '{{ app.request.get("demo") }}');
        googletag.defineSlot('/60988533/Rio_Section_320x50', [320, 50], 'div-gpt-ad-1485772432963-5').addService(googletag.pubads()).setTargeting('demo', '{{ app.request.get("demo") }}');
        googletag.defineSlot('/60988533/Rio_SS_Mrec', [300, 250], 'div-gpt-ad-1485772432963-6').addService(googletag.pubads()).setTargeting('demo', '{{ app.request.get("demo") }}');
        googletag.defineSlot('/60988533/Rio_Story_LB1', [728, 90], 'div-gpt-ad-1485772432963-7').addService(googletag.pubads()).setTargeting('demo', '{{ app.request.get("demo") }}');
        googletag.defineSlot('/60988533/Rio_Story_LB2', [728, 90], 'div-gpt-ad-1485772432963-8').addService(googletag.pubads()).setTargeting('demo', '{{ app.request.get("demo") }}');
        googletag.defineSlot('/60988533/Rio_Story_Mrec1', [300, 250], 'div-gpt-ad-1485772432963-9').addService(googletag.pubads()).setTargeting('demo', '{{ app.request.get("demo") }}');
        googletag.defineSlot('/60988533/Rio_Story_Mrec2', [300, 250], 'div-gpt-ad-1485772432963-10').addService(googletag.pubads()).setTargeting('demo', '{{ app.request.get("demo") }}');
        googletag.pubads().enableSingleRequest();
        googletag.pubads().collapseEmptyDivs();
        googletag.pubads().enableSyncRendering();
        googletag.enableServices();
      });
    </script>

    <!-- Qlitics Snippet -->
    <script>
      window.qlitics=window.qlitics||function(){(qlitics.q=qlitics.q||[]).push(arguments);};
      qlitics('init');
      qlitics('track', 'page-view', {'page-type': "{{ page.type }}" });
    </script>
    <script async src='/qlitics.js'></script>
    <!-- End Qlitics Snippet -->
  </head>
  <body class='page'>
    <script type="text/javascript">var qtReady = qtReady || [];</script>

    <div class="header">
      {% include "/shared/header" %}
    </div>

    <div class="content {{ page.type }}">
      {% block content %}{% endblock %}
    </div>

    <div class="footer">
      {% include "/shared/footer" %}
    </div>

    <script type="text/javascript">
      var qtConfig = {
          "image-cdn": "{{config("quintype.image-cdn")}}",
          "publisher-name": "{{config("quintype.publisher-name")}}",
          "polltype-host": "{{config['polltype-host']}}"
      };
    </script>

    <script type="text/javascript">
      app.initQtReady();
    </script>

    <script type="text/javascript">
      var parentElement = '.js-desktop-submenu';
      var targetElement = '.js-submenu-stories-container';
      var triggerElement = '.js-desktop-submenu__item';
      var fields = "id,headline,slug,url,hero-image-s3-key,hero-image-metadata,first-published-at,last-published-at,alternative,published-at,author-name,author-id,sections,story-template,summary,metadata,hero-image-attribution,cards,subheadline,authors";
      var params = {"limit": 8, "story-group": "top", "fields": fields}
      qtReady.push(function() { app.subSectionStories(parentElement, triggerElement, targetElement, params); })
    </script>
  </body>
</html>
