<section class="top">
    <div class="leftcontainer">
        {% for story in stories | slice(0, 1) %}
            <a href="{{story['slug']}}">
                <div class="figcontainer">
                    <h6 class="sectionname__display topdesk">{{story['sections'][0]['name']}}</h6>
                    <div class="topcontentwrap">
                        <h6 class="sectionname__display topmob">{{story['sections'][0]['name']}}</h6>
                        <div class="titletop_big">{{ shorthead(story["headline"]) }}</div>
                        <div class="textbottom_left">
                            {% if story['metadata']['story-attributes']['partnerpublisher'][0] %}
                                <div class="imagesd">
                                    <img src="{{  get_logo(story['metadata']['story-attributes']['partnerpublisher'][0], 'Logo' ) }}"/>
                                </div>
                            {% endif %}
                            <span>
                                <h5 class="textbottom__article__name">
                                    <a href="/author/{{story['author-id']}}">{{story["author-name"]}}</a>
                                </h5>
                            </span>
                        </div>
                        <div class="sourcetop_left">{{ story["hero-image-attribution"] | raw }}</div>
                    </div>
                    <div style="width:100%;">
                        <figure id="hero_image">
                            <a href="{{story['slug']}}">
                                <img src="{{focusedImageUrl(story['hero-image-s3-key'], config['image-cdn'], [16,9], story['hero-image-metadata'],{w:700} )}}"/>
                            </a>
                        </figure>
                    </div>
                </div>
            </a>
        {% endfor %}
    </div>
    <div class="rightcontainer">
        {% for story in stories | slice(1, 3) %}
            <div class="rightitem">
                <a href="{{story['slug']}}">
                    <div class="figcontainer">
                        <h6 class="sectionname__display">{{story['sections'][0]['name']}}</h6>
                        <div class="titletop_small">{{ shorthead(story["headline"]) }}</div>
                        <div class="sourcetop">{{ story["hero-image-attribution"] | raw}}</div>
                        <figure>
                          <img src="{{focusedImageUrl(story['hero-image-s3-key'], config['image-cdn'], [16,9], story['hero-image-metadata'],{w:300})}}"/>
                        </figure>
                    </div>
                </a>
            </div>
        {% endfor %}
    </div>
</section>

<section>
    <div class="gallery-fix-div">
        {% for story in stories | slice(4, 3) %}
            <article>
                <div class="wrap border">
                    <div class="figcontainer">
                        <h6 class="sectionname__display">{{story['sections'][0]['name']}}</h6>
                        <a href="/{{story['slug']}}">
                            <figure>
                                <img src="{{focusedImageUrl(story['hero-image-s3-key'], config['image-cdn'], [16,9], story['hero-image-metadata'], {w: 400})}}"/>
                            </figure>
                            <div class="sourcefrom">{{ story["hero-image-attribution"] | raw }}</div>
                        </a>
                        <span class="story-template-type">
                          {{story['story-template']}}
                        </span>
                    </div>

                    <div class="textwrap">
                        <a href="/{{story['slug']}}">
                            <div class="article__title">
                                {{  shorthead(story["headline"]) }}
                            </div>
                            <div class="article__desciption">
                                {{ story["subheadline"] | striptags | slice(0,100) | raw  }}
                            </div>
                        </a>
                    </div>
                    {% include "story/byline" %}
                </div>
            </article>
        {% endfor %}
    </div>
</section>

<section>
    <div class="story2-banner1">
        <div class="leftbig">
            <figure>
                <a href="{{banners[0]['slug']}}">
                    <img src="{{focusedImageUrl(banners[0]['hero-image-s3-key'], config['image-cdn'], [16,4.45], banners[0]['hero-image-metadata'], {w:720})}}"/>
                </a>
            </figure>
        </div>

        <div class="rightsmall">
            {% for story in stories | slice(7, 1) %}
                <a href="/{{story['slug']}}">
                    <figure>
                        <span class="sectionname__display">{{story['sections'][0]['name']}}</span>
                        <div class="titletop_small">
                            {{ shorthead(story["headline"]) }}</div>
                        <div class="sourcetop">{{ story["hero-image-attribution"] | raw }}</div>
                        <img src="{{focusedImageUrl(story['hero-image-s3-key'], config['image-cdn'], [16,9], story['hero-image-metadata'], {w:400})}}"/>
                    </figure>
                </a>
            {% endfor %}
        </div>
    </section>

